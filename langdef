cmplx-expr	: expr (COMMA set-expr) SEMI_COLON

set-expr	: (WORD EQ)? logic-expr

logic-expr	: comp-expr ((AND|OR) comp-expr)*
		: NOT logic-expr

comp-expr	: arith-expr (EQL arith-expr)*
		: arith-expr (NEQ arith-expr)*
		: arith-expr ((LS|LSE) arith-expr)*
		: arith-expr ((GR|GRE) arith-expr)*
		
arith-expr	: term ((PLUS|MINUS) term)*

term		: factor ((DIV|MUL) factor)*

factor		: (PLUS|MINUS) factor
		: atom

atom		: NUMBER | INT
		: WORD
		: LBRACK set-expr RBRACK
